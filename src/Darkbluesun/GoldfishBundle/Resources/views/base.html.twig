{% extends app.request.xmlHttpRequest ? "ajax.html.twig" : "base.html.twig" %}
{% if edit_form is defined %}
{% form_theme edit_form 'form_table_layout.html.twig' %}
{% endif %}
{% if form is defined %}
{% form_theme form 'form_table_layout.html.twig' %}
{% endif %}
{% set route = app.request.attributes.get('_route') %}
{% block nav %}
{% include "DarkbluesunGoldfishBundle::nav.html.twig" %}
{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" type="text/css" href="{{asset('assets/vendor.css')}}">
{% stylesheets filter='?uglifycss' filter='cssrewrite' output='assets/main.css' 'bundles/darkbluesungoldfish/css/goldfish.css' %}
        <link rel="stylesheet" type="text/css" href="{{asset_url}}">
{% endstylesheets %}
{% endblock %}

{% block javascripts %}

  <!--javascript libraries-->
 <script type="text/javascript" src="{{asset('assets/vendor.js')}}"></script>
{% javascripts filter='?uglifyjs2' output='assets/main.js'
'bundles/darkbluesungoldfish/js/goldfish.js' 
'bundles/darkbluesungoldfish/js/timer.js'  %}
  <script type="text/javascript" src="{{asset_url}}"></script>
{% endjavascripts %}
{% endblock %}