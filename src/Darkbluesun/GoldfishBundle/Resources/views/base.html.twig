{% extends "base.html.twig" %}
{% if edit_form is defined %}
{% form_theme edit_form 'form_table_layout.html.twig' %}
{% endif %}
{% set route = app.request.attributes.get('_route') %}
{% block nav %}
{% include "DarkbluesunGoldfishBundle::nav.html.twig" %}
{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/3.1.3/css/bootstrap-datetimepicker.min.css">
{% stylesheets 'bundles/darkbluesungoldfish/css/goldfish.css' %}
        <link rel="stylesheet" type="text/css" href="{{asset_url}}">
{% endstylesheets %}
{% endblock %}

{% block javascripts %}

  <!--moment js used by the datepicker-->
  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js"></script>

  <!--bootstrap-datetimepicker for date/time picker popouts-->
  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/3.1.3/js/bootstrap-datetimepicker.min.js"></script>

  <!--site javascript-->
{% javascripts 'bundles/darkbluesungoldfish/js/*' %}
  <script type="text/javascript" src="{{asset_url}}"></script>
{% endjavascripts %}
{% endblock %}