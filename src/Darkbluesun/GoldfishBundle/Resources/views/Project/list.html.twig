    <table class="table table-striped records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Duedate</th>
                <th>Budget</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('project_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.dueDate.format('d/m/y') }}</td>
                <td>{{ entity.budget }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>